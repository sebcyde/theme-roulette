{
	"publisher:": "JTSC25",
	"name": "theme-roulette",
	"displayName": "Theme Roulette",
	"description": "A random theme selector built on the Doki Theme extension",
	"version": "1.0.0",
	"engines": {
		"vscode": "^1.75.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/https://github.com/sebcyde/theme-roulette"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:theme-roulette.helloWorld"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "theme-roulette.timer",
				"title": "Start Timed Roulette"
			},
			{
				"command": "theme-roulette.stop",
				"title": "Stop Roulette"
			},
			{
				"command": "theme-roulette.spin",
				"title": "Spin Roulette"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^8.0.1",
		"@types/mocha": "^10.0.1",
		"@types/node": "16.x",
		"@types/vscode": "^1.75.0",
		"@typescript-eslint/eslint-plugin": "^5.49.0",
		"@typescript-eslint/parser": "^5.49.0",
		"@vscode/test-electron": "^2.2.2",
		"eslint": "^8.33.0",
		"glob": "^8.1.0",
		"mocha": "^10.1.0",
		"typescript": "^4.9.4"
	},
	"dependencies": {
		"@vscode/vsce": "^2.17.0",
		"vsce": "^2.15.0"
	}
}
